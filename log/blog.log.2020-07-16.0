```java
class Solution {
    public TreeNode buildTree(int[] preorder, int[] inorder) {
        if(preorder.length==0 || inorder.length==0) {
            return null;
        }
        //æ ¹æ®å‰åºæ•°ç»„çš„ç¬¬ä¸€ä¸ªå…ƒç´ ï¼Œå°±å¯ä»¥ç¡®å®šæ ¹èŠ‚ç‚¹
        TreeNode root = new TreeNode(preorder[0]);
        for(int i=0;i<preorder.length;++i) {
            //ç”¨preorder[0]åŽ»ä¸­åºæ•°ç»„ä¸­æŸ¥æ‰¾å¯¹åº”çš„å…ƒç´ 
            if(preorder[0]==inorder[i]) {
                //å°†å‰åºæ•°ç»„åˆ†æˆå·¦å³ä¸¤åŠï¼Œå†å°†ä¸­åºæ•°ç»„åˆ†æˆå·¦å³ä¸¤åŠ
                //ä¹‹åŽé€’å½’çš„å¤„ç†å‰åºæ•°ç»„çš„å·¦è¾¹éƒ¨åˆ†å’Œä¸­åºæ•°ç»„çš„å·¦è¾¹éƒ¨åˆ†
                //é€’å½’å¤„ç†å‰åºæ•°ç»„å³è¾¹éƒ¨åˆ†å’Œä¸­åºæ•°ç»„å³è¾¹éƒ¨åˆ†
                int[] pre_left = Arrays.copyOfRange(preorder,1,i+1);
                int[] pre_right = Arrays.copyOfRange(preorder,i+1,preorder.length);
                int[] in_left = Arrays.copyOfRange(inorder,0,i);
                int[] in_right = Arrays.copyOfRange(inorder,i+1,inorder.length);
                root.left = buildTree(pre_left,in_left);
                root.right = buildTree(pre_right,in_right);
                break;
            }
        }
        return root;
    }
